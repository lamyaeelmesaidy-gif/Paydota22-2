{
  "expo": {
    "name": "PayDota Banking",
    "slug": "paydota-banking",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./client/public/favicon.ico",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./client/public/favicon.ico",
      "resizeMode": "contain",
      "backgroundColor": "#0066CC"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.paydota.banking",
      "infoPlist": {
        "UIBackgroundModes": ["background-fetch"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./client/public/favicon.ico",
        "backgroundColor": "#0066CC"
      },
      "package": "com.paydota.banking",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./client/public/favicon.ico",
      "bundler": "webpack",
      "build": {
        "babel": {
          "include": [
            "client/src"
          ]
        }
      }
    },
    "platforms": ["ios", "android", "web"],
    "scheme": "paydota",
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-splash-screen"
    ]
  }
}